{
  "name": "resin-hooks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r --filter './packages/*' build",
    "build:hooks": "pnpm --filter @resin-hooks/core build",
    "build:watch": "pnpm --filter @resin-hooks/core dev",
    "clean": "pnpm -r --filter './packages/*' exec -- node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "docs:dev": "dumi dev",
    "rspress:dev": "rspress dev",
    "rspress:build": "pnpm play:build && rspress build",
    "rspress:preview": "rspress preview",
    "play:dev": "pnpm --filter @resin-hooks/play dev",
    "play:build": "pnpm --filter @resin-hooks/play build",
    "play:preview": "pnpm --filter @resin-hooks/play preview"
  },
  "workspaces": [
    "packages/**"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "dumi": "^2.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rc-util": "^5.44.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.55.1",
    "rollup-plugin-delete": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rspress": "^1.47.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "lodash-es": "^4.17.23"
  }
}
